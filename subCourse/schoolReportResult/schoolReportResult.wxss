/* subCourse/schoolReportResult/schoolReportResult.wxss */
@import "../../assets/css/common.wxss";

/* 返回 */
.back {
  position: fixed;
  left: 14rpx;
  top: 10rpx;
  width: calc(100% - 14rpx);
  z-index: 50;
}

.back-module {
  position: relative;
  width: 100vw;
  left: -14rpx;
}

.back image {
  width: 64rpx;
  height: 64rpx;
  margin-left: 14rpx;
}

.back-module text {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 600;
  color: #000;
  line-height: 38rpx;
}

.main {}

.img {
  padding-top: 260rpx;
  text-align: center;
}

.img image {
  width: 266rpx;
  height: 200rpx;

}

.title {
  text-align: center;
  font-size: 52rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 600;
  color: #000000;
  text-align: center;
  line-height: 52rpx;
  padding-top: 66rpx;
  padding-bottom: 40rpx;
}

.camp-name {
  padding-top: 40rpx;
  padding-bottom: 20rpx;
}

.desc {
  font-size: 36rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #717171;
  line-height: 36rpx;
  text-align: center;
}

.button {
  width: calc(100% - 120rpx);
  text-align: center;
  line-height: 96rpx;
  height: 96rpx;
  border-radius: 999rpx;
  font-size: 40rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 600;
  color: #FFFFFF;
  position: fixed;
  left: 50%;
  transform: translateX(-50%);
  bottom: 100rpx;
}