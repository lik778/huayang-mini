/* subCourse/descriptionOfofflineTrainCourse/descriptionOfofflineTrainCourse.wxss */
.container image {
  width: 100%;
  padding-bottom: 120rpx;
  vertical-align: bottom;
}

.concact-module {
  width: 100vw;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
}

.concact-button {
  width: calc(100% - 60rpx);
  height: 96rpx;
  border-radius: 999rpx;
  text-align: center;
  font-size: 20px;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 600;
  color: rgba(255, 255, 255, 1);
  background-color: #ff5544;
  line-height: 96rpx;
}