.invite-page {
    min-height: 100vh;
    background: white;
    color: white;
}
.header {
    position: relative;
    padding-top: 178rpx;
    box-sizing: border-box;
    height: 672rpx;
    text-align: center;
    background: url("https://huayang-img.oss-cn-shanghai.aliyuncs.com/1600235547rgeTbk.jpg") no-repeat center;
    background-size: cover;
}
.view-01 {
    font: bold 56rpx/80rpx PingFangSC;
}
.txt-01 {
    font: bold 73rpx/80rpx PingFangSC;
    color: #FFEDBD;
}
.view-02 {
    margin-top: 6rpx;
    font: 40rpx/56rpx PingFangSC;
}
.conclude-box {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    margin-top: 44rpx;
    font: 500 40rpx/50rpx PingFangSC;
}
.done-image {
    width: 160rpx;
    height: 160rpx;
    margin-bottom: 20rpx;
    font-size: 0;
}
.process {
    position: relative;
    width: 588rpx;
    margin: 86rpx auto 0;
}
.line {
    width: 100%;
    height: 8rpx;
    background: #FFEDBD;
}
.line::after {
    content: "";
    width: 100%;
    height: 100%;
    background: #AF0404;
}
.dot {

}
.process-icon {
    position: absolute;
    top: -44rpx;
    right: -14rpx;
    width: 92rpx;
    height: 88rpx;
    font-size: 0;
}
.no {
    margin-top: 48rpx;
    text-align: center;
    font: 36rpx/36rpx PingFangSC;
}
.wx-btn {
    position: absolute;
    bottom: -58rpx;
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    justify-content: center;
    align-items: center;
    width: 418rpx;
    height: 108rpx;
    box-sizing: border-box;
    border-radius: 999rpx;
    border: 6rpx solid #fff;
    font: 500 40rpx/56rpx PingFangSC;
    background: #00BB66;
}
.wx-btn:active {
    opacity: 0.98;
}
.wx-icon {
    width: 64rpx;
    height: 50rpx;
    font-size: 0;
    margin-right: 20rpx;
}
.section {
    margin-top: 122rpx;
    font: 40rpx/40rpx PingFangSC;
    color: black;
}
.view-03 {
    display: flex;
    justify-content: center;
    align-items: center;
}
.view-03 > text {
    margin: 0 20rpx;
}
.sub-line {
    width: 160rpx;
    height: 2rpx;
    background: rgba(0,0,0,.2);
}
.invited-user {
    padding: 60rpx 38rpx 0;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
}
.invited-user-avatar {
    width: 64rpx;
    height: 64rpx;
    margin: 20rpx 10rpx 0;
    border-radius: 999rpx;
    font-size: 0;
    background: #eee;
}
