/* components/navibar.wxss */
.navibar {
  display: flex;
  align-items: center;
  height: 44px;
  line-height: 44px;
  position: relative;
  position: fixed;
  width: 100vw;
  left: 0;
  top: 0;
  /* background: #fff; */
  z-index: 2000;
}

.back-module {
  display: flex;
  align-items: center;
  width: 92px;
  height: 30px;
  border: 0.5px solid #DDDDDD;
  border-radius: 999rpx;
  margin-left: 8px;
  background: rgba(255, 255, 255, 0.6);
}

.back-icon {
  display: flex;
  align-items: center;
  height: 100%;
  padding: 0 34rpx;
}

.back-icon image {
  width: 18rpx;
  height: 32rpx;
}

.line-icon {
  width: 1px;
  height: 18.5px;
  background: rgba(0, 0, 0, 0.2);
}

.back-index {
  display: flex;
  align-items: center;
  padding: 0 26rpx;
  height: 100%;

}

.back-index image {
  width: 32rpx;
  height: 32rpx;
}

.title {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  font-size: 17px;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 600;
  color: rgba(0, 0, 0, 1);
  line-height: 19px;
}