/* mine/withdrawResult/withdrawResult.wxss */
.main {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 356rpx;
}

.main-title {
  font-size: 34px;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(5, 197, 98, 1);
  line-height: 34px;
  padding-bottom: 80rpx;
}

.money-line text:first-child {
  font-size: 20px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 20px;
}

.money-line text:last-child {
  font-size: 34px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
  line-height: 34px;
}

.note {
  font-size: 18px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 18px;
  padding-top: 40rpx;
}