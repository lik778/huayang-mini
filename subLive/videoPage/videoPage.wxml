<!--subLive/review/review.wxml-->
<view class="review">
  <view class="navigation">
    <naviBar title="花样百姓+" titleColor="transparent" color="transparent"></naviBar>
  </view>
  <video
    custom-cache="{{false}}"
    object-fit="contain"
    enable-play-gesture="{{true}}"
    enable-auto-rotation="{{true}}"
    play-btn-position="top"
    src="{{link}}"
    class="video-container {{adapter ? 'adapter' : ''}}"></video>
</view>
